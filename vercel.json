{
    "crons": [
        {
            "path": "/api/cron?action=log",
            "schedule": "1 8 * * *"
        }
    ],
    "headers": [
        {
            "source": "/api/firebase/user",
            "headers": [
                {
                    "key": "Access-Control-Allow-Credentials",
                    "value": "true"
                },
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "https://zelda.walcron.com"
                },
                {
                    "key": "Access-Control-Allow-Methods",
                    "value": "GET,DELETE,PATCH,POST,PUT"
                },
                {
                    "key": "Access-Control-Allow-Headers",
                    "value": "Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version"
                }
            ]
        }
    ]
}